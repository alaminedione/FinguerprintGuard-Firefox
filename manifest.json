{
    "manifest_version": 2,
    "name": "FingerprintGuard",
    "version": "2.0.0",
    "description": "Protege la vie priv√©e des utilisateurs en usurpant les informations du navigateur. (gratuite et open source)",
    "applications": {
        "gecko": {
            "id": "fingerprintguard@fingerprintjs.com"
        }
    },
    "permissions": [
        "storage",
        "scripting",
        "declarativeNetRequest",
        "webNavigation",
        "tabs",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "FingerprintGuard"
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": true
    }
}
